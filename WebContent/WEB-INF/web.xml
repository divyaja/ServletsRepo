<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  
  <servlet>
  <servlet-name>abc</servlet-name>
  <servlet-class>com.servlets.demo.divya.AddServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>abc</servlet-name>
  <url-pattern>/add</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>asd</servlet-name>
  <servlet-class>com.servlets.demo.divya.SqServlet</servlet-class>
  </servlet>
    
  <servlet-mapping>
  <servlet-name>asd</servlet-name>
  <url-pattern>/sq</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>asdf</servlet-name>
  <servlet-class>com.servlets.demo.divya.MyServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>asdf</servlet-name>
  <url-pattern>/home</url-pattern>
  </servlet-mapping>
  
 

  
  <context-param>
  <param-name>name</param-name>
  <param-value>Divya</param-value>
